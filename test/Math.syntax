
syntax com.phenan.arith.MathDSL (program) {
  program = stmts | stmt    ;
  stmts = stmt program      ;
  stmt = expr "end"         ;
  expr = add | sub | term   ;
  add = expr "+" term       ;
  sub = expr "-" term       ;
  term = mul | div | factor ;
  mul = term "*" factor     ;
  div = term "/" factor     ;
  factor = paren | num      ; 
  paren = "(" expr ")"      ;
  num = int                 ; 
}
